<div class="wrapper">
  <%= render "shared/header" %>

  <div class="form-title">
    <h2>Sign up</h2>
  </div>
  
  <div class="form-main">
    <%= form_with model: @profile, url: profiles_path, id: 'new_user', class: 'registration-main', local: true do |f| %>

      <%= render partial: "shared/profile_form", locals: { f: f } %>

      <div class="actions">
        <%= f.submit "Submit" %>
      </div>
    <% end %>

    <%#= render "devise/shared/links" %>
  </div>

  <%= render "shared/footer" %>

</div>
